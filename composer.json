{
    "name": "ze/laravel-jsonrpc-client",
    "description": "rpc client base on json-rpc with http",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Ze\\JsonRPCClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ze",
            "email": "954489346zyz@sina.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": ">6.3",
        "laravel/framework": ">=5.5",
        "php": ">=7.0.0",
        "ext-json": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RPCClientProvider"
            ],
            "aliases":
            {
                "RPC": "RPCClient"
            }
        }
    }
